{
  "name": "@uxland/uxl-ui-skeleton",
  "version": "0.0.0-development",
  "description": "UXLand - UI Skeleton",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
  "keywords": [
    "polymer",
    "web components"
  ],
  "homepage": "https://github.com/uxland/uxl-ui-skeleton#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/uxland/uxl-ui-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/uxland/uxl-ui-skeleton/issues"
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "web-component-analyzer": "^0.1.21"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/polymer": "^1.2.4",
    "@types/sinon": "^5.0.1",
    "@types/webdriverio": "^4.10.3",
    "@webcomponents/custom-elements": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "allure-commandline": "^2.7.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.1",
    "css-modules-typescript-loader": "^2.0.4",
    "dts-generator": "^2.1.0",
    "ejs-loader": "0.3.1",
    "esm": "^3.0.72",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "gulp": "^4.0.0",
    "gulp-run": "^1.7.1",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.12.0",
    "lit-scss-loader": "^1.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "mutation-observer": "^1.0.3",
    "ncp": "^2.0.0",
    "node-sass": "^4.11.0",
    "nsp": "^2.6.3",
    "raf": "^3.4.0",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.3",
    "run-sequence": "^2.1.0",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "selenium-standalone": "^6.15.1",
    "semantic-release": "^16.0.0-beta.22",
    "sinon": "^6.0.0",
    "to-string-loader": "^1.1.5",
    "travis-deploy-once": "^5.0.7",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6",
    "wct-browser-legacy": "^1.0.0",
    "wdio-allure-reporter": "^0.7.0",
    "wdio-dot-reporter": "0.0.10",
    "wdio-mocha-framework": "^0.6.3",
    "wdio-selenium-standalone-service": "0.0.10",
    "webdriverio": "^4.13.2",
    "webpack": "4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "scripts": {
    "deps:delete": "npx rimraf node_modules package-lock.json",
    "deps:install": "npm install",
    "deps:refresh": "npm run deps:delete && npm run deps:install",
    "build:es": "tsc -p tsconfig.build.json --outDir es  ",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib --module commonjs --declaration",
    "build": "npm run build:es && npm run build:cjs",
    "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
    "test:ui": "polymer test --config-file wct.config.json",
    "test:unit": "node --experimental-modules node_modules/mocha/bin/_mocha -r esm --ui tdd --reporter mochawesome test/unit",
    "tdd": "node --experimental-modules node_modules/mocha/bin/_mocha --watch -r esm --ui tdd --reporter mochawesome test/unit",
    "dist:copy-resources:es": "cpx \"src/**/*.{gif,png,jpg,jpeg,ico,pdf,svg,scss,d.ts}\" ./es",
    "dist:copy-resources:lib": "cpx \"src/**/*.{gif,png,jpg,jpeg,ico,pdf,svg,scss,d.ts}\" ./lib",
    "dist": "npm run dist:copy-resources:es && npm run dist:copy-resources:lib && npm run build",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "semantic-release:local": "npm run dist && semantic-release --no-ci --debug",
    "postsemantic-release:local": "git checkout -- package*",
    "test": "echo \"Error: no test specified\" && exit 0",
    "webpack:build": "webpack --progress",
    "webpack:build:prod": "webpack --env.NODE_ENV=production --optimize-minimize --progress",
    "serve": "webpack-dev-server --hot --inline --progress",
    "delete:transpiledjs": "npx rimraf src/**/*.js src/**/*.js.map demo/index.js demo/index.js.map",
    "analyce-wc": "wca analyze src --format markdown"
  },
  "license": "Apache-2.0",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  }
}
